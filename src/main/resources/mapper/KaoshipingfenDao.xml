 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.KaoshipingfenView" >

		SELECT  kaoshipingfen.* FROM kaoshipingfen  kaoshipingfen 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.KaoshipingfenView" >
